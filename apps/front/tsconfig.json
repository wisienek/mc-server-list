{
  "extends": "../../tsconfig.base.json",
  "files": [],
  "include": [
    "../../apps/front/.next/types/**/*.ts",
    "svgr.d.ts",
    ".next/types/**/*.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
